 [index, setIndex] = React.useState(0)\n\n  useEffect(() => {\n    const lastIndex = people.length - 1\n    if (index < 0) {\n      setIndex(lastIndex)\n    }\n    if (index > lastIndex) {\n      setIndex(0)\n    }\n  }, [index, people])\n\n  useEffect(() => {\n    let slider = setInterval(() => {\n      setIndex(index + 1)\n    }, 5000)\n    return () => {\n      clearInterval(slider)\n    }\n  }, [index])\n\n  return (\n    <section className=\"section\">\n      <div className=\"title\">\n        <h2>Reviews</h2>\n      </div>\n      <div className=\"section-center\">\n        {people.map((person, personIndex) => {\n          const { id, image, name, title, quote } = person\n\n          let position = 'nextSlide'\n          if (personIndex === index) {\n            position = 'activeSlide'\n          }\n          if (\n            personIndex === index - 1 ||\n            (index === 0 && personIndex === people.length - 1)\n          ) {\n            position = 'lastSlide'\n          }\n\n          return (\n            <article className={position} key={id}>\n              <img src={image} alt={name} className=\"person-img\" />\n              <h4>{name}</h4>\n              <p className=\"title\">{title}</p>\n              <p className=\"text\">{quote}</p>\n              <FaQuoteRight className=\"icon\" />\n            </article>\n          )\n        })}\n        <button className=\"prev\" onClick={() => setIndex(index - 1)}>\n          <FiChevronLeft />\n        </button>\n        <button className=\"next\" onClick={() => setIndex(index + 1)}>\n          <FiChevronRight />\n        </button>\n      </div>\n    </section>\n  )\n}\n\nexport default Testimonials\n","/Users/marinbargan/Desktop/Teach English Through Football/src/index.js",[],"/Users/marinbargan/Desktop/Teach English Through Football/src/components/CourseOptions/index.js",["315"],"import React from 'react'\r\nimport './CourseElements.css'\r\nimport DownArrow from '../../images/down.png'\r\nimport { use